{
  "name": "zemptime.pr-stats",
  "description": "pr-stats",
  "version": "0.0.0",
  "author": "zemptime",
  "repository": {
    "type": "git",
    "url": "https://github.com/ZempTime/pr-stats/"
  },
  "license": "MIT",
  "devDependencies": {
    "@graphql-codegen/add": "^2.0.2",
    "@graphql-codegen/cli": "1.21.7",
    "@graphql-codegen/introspection": "1.18.2",
    "@graphql-codegen/typescript": "1.23.0",
    "@graphql-codegen/typescript-document-nodes": "^1.17.16",
    "@graphql-codegen/typescript-operations": "1.18.4",
    "aha-cli": "^1.9.2"
  },
  "ahaExtension": {
    "cspSources": [],
    "contributes": {
      "commands": {
        "refreshPullRequest": {
          "title": "PR Stats: Refresh Pull Request",
          "entryPoint": "src/commands/refreshPullRequest.js"
        }
      },
      "events": {
        "observer": {
          "title": "Github First Review Observer",
          "entryPoint": "src/events/github.pr.js",
          "handles": [
            "aha-develop.github.pr.review_requested",
            "aha-develop.github.pr.review_request_removed",
            "aha-develop.github.pull_request_review.submitted",
            "aha-develop.github.pull_request_review.dismissed"
          ]
        }
      },
      "settings": {
        "firstReviews": {
          "title": "(to be removed shortly) First Review Team Filters",
          "description": "[Team identifier,ghname1,ghname2,...] to track time to first review.",
          "scope": [
            "account"
          ],
          "type": "string",
          "array": true,
          "default": [
            "Conversion,zemptime"
          ]
        },
        "cutoffDate": {
          "title": "Cutoff Date",
          "scope": [
            "account"
          ],
          "type": "string",
          "description": "Pull requests between now and this date will be searched for. Ensure this works with `Date.parse()`. e.g. '2021-06-01'."
        },
        "repos": {
          "title": "Related repositories",
          "scope": [
            "account"
          ],
          "type": "string",
          "array": true,
          "description": "List of the GitHub repositories that should be searched for pull request metrics. e.g. 'rails/rails'."
        }
      },
      "views": {
        "firstReviewPage": {
          "title": "First Reviews",
          "host": "page",
          "entryPoint": "src/views/firstReviewPage.js",
          "location": {
            "menu": "Document"
          }
        }
      }
    }
  },
  "dependencies": {
    "gql-tag": "^1.0.1",
    "graphql": "^15.5.1",
    "xstate": "^4.23.0"
  },
  "scripts": {
    "codegen": "graphql-codegen --config codegen.yml"
  }
}
